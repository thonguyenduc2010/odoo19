<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="ai_agent_object_reference_form_view" model="ir.ui.view">
        <field name="name">AI Agent Object Reference Form View</field>
        <field name="model">ai.agent.object.reference</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="sequence"/>
                            <field name="reference_id"/>
                            <field name="reference_name"/>
                            <field name="output"/>
                        </group>
                        <group>
                            <field name="agent_response_history_step_id"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="ai_agent_object_reference_tree_view" model="ir.ui.view">
       <field name="name">AI Agent Object Reference Tree View</field>
       <field name="model">ai.agent.object.reference</field>
       <field name="arch" type="xml">
           <list>
               <field name="sequence"/>
               <field name="output"/>
               <field name="reference_name" />
               <field name="reference_id"/>
               <field name="agent_response_history_step_id"/>
           </list>
       </field>
    </record>

    <record id="action_ai_agent_object_reference" model="ir.actions.act_window">
       <field name="name">AI Agent Object Reference</field>
       <field name="res_model">ai.agent.object.reference</field>
       <field name="view_mode">list,form</field>
       <field name="help" type="html">
           <p class="o_view_nocontent_smiling_face">
               AI Agent Object Reference
           </p>
       </field>
    </record>

</odoo>