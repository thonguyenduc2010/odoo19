<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_ai_agent_product_quantity_wizard_form" model="ir.ui.view">
        <field name="name">product.quantity.wizard.form</field>
        <field name="model">product.quantity.wizard</field>
        <field name="arch" type="xml">
            <form string="Add Products">
                <sheet>
                    <group>
                        <field name="agent_id"  invisible="1"/>
                        <field name="line_ids" no_label="1" string="">
                            <tree editable="bottom">
                                <field name="product_id"/>
                                <field name="quantity"/>
                            </tree>
                        </field>
                    </group>
                </sheet>
                <footer>
                    <button name="action_confirm" string="Confirm and Run" type="object" class="btn-primary"/>
                    <button string="Cancel" special="cancel" class="btn-secondary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>